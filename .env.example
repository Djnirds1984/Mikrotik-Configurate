# MikroTik Cloud Manager Environment Variables

## Frontend (.env.local in client directory)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

## Backend (wrangler.toml in worker directory)
[vars]
SUPABASE_URL = "your_supabase_project_url"
SUPABASE_SERVICE_KEY = "your_supabase_service_role_key"

## Development URLs
FRONTEND_URL=http://localhost:3000
BACKEND_URL=http://localhost:8787

## MikroTik Router Defaults
DEFAULT_API_PORT=8728
DEFAULT_USERNAME=admin

## Security
JWT_SECRET=your_jwt_secret_key
ENCRYPTION_KEY=your_encryption_key_for_router_passwords